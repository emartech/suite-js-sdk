  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "isolatedModules": true,
    "module": "commonjs",
    "noUncheckedSideEffectImports": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": false
  }
}
